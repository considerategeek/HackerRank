#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include <iomanip>
using namespace std;


int main(){
   float  a,b;
   scanf("%f",&a);
   scanf("%f",&b);
   float res,res1;
   res=160+40.0*(a+pow(a,2));
   res1=128+40.0*(b+pow(b,2));
   cout << setprecision(7)<< res << endl;
   cout << setprecision(7)<< res1 << endl;
   return 0;
}
